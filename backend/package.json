{
  "name": "agrichain-backend",
  "version": "1.0.0",
  "description": "Backend API for AgriChain supply chain platform",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "seed": "node scripts/seed.js",
    "seed:clear": "node scripts/seed.js --clear",
    "seed:large": "node scripts/seed.js --clear --count=3",
    "seed:products": "node scripts/seedProducts.js",
    "seed:all": "node scripts/seedAllData.js",
    "update:images": "node scripts/updateProductImages.js"
  },
  "keywords": [
    "blockchain",
    "agriculture",
    "supply-chain",
    "api"
  ],
  "author": "AgriChain Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ethers": "^6.10.0",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "form-data": "^4.0.0",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "redis": "^4.6.12",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.1.0",
    "@types/jest": "^29.5.11",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
